#
# ~/.bashrc
#

# If not running interactively, don't do anything
[[ $- != *i* ]] && return

alias ls='eza --git'
alias grep='grep --color=auto'
alias diff="diff --color=auto"
alias stow='stow --dotfiles -v'
alias vim='nvim'

PS1='[\W]\$ '
EDITOR='/usr/bin/nvim'

eval "$(zoxide init bash)"
eval "$(fzf --bash)"
